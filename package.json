{
    "name": "simple-artisan",
    "displayName": "Simple Artisan",
    "description": "A simple Laravel Artisan gateway extension.",
    "version": "0.0.1",
    "publisher": "soldenburg",
    "engines": {
        "vscode": "^1.19.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.artisan",
        "onCommand:extension.artisan-migrate-fresh-seed"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.artisan",
                "title": "php artisan"
            },
            {
                "command": "extension.artisan-migrate-fresh-seed",
                "title": "php artisan migrate:fresh --seed"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
